        <servlet-name>ViewApplicationWithdrawalServlet</servlet-name>
        <url-pattern>/ViewApplicationWithdrawalServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StartWithDrawApplicationServlet</servlet-name>
        <url-pattern>/StartWithDrawApplicationServlet.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
